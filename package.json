{
  "name": "jedlik-typescript-vscode-template",
  "version": "1.0.0",
  "description": "Jedlik project template for using TypeScript and Node.js in VS Code.",
  "engines": {
    "node": "11.x"
  },
  "main": "app.js",
  "repository": {
    "private": true,
    "type": "git",
    "url": "https://github.com/nitslaszlo/JedlikTsTemplate"
  },
  "scripts": {
    "start": "node ./js/app.js",
    "start-debug": "node --debug=5858 ./js/app.js",
    "install:nodemodules": "npm install",
    "compile": "tsc",
    "nodemon": "nodemon --inspect -q ./js/app.js",
    "kill": "taskkill /im node.exe /F"
  },
  "author": "Nits László",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.7.4"
  }
}
